{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Import",
            "type": "shell",
            "command": "${workspaceFolder}\\import_addon.bat",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Package",
            "type": "shell",
            "command": "${workspaceFolder}\\package_addon.bat"
        },
        {
            "label": "Remove",
            "type": "shell",
            "command": "${workspaceFolder}\\remove_addon.bat"
        }
    ]
}